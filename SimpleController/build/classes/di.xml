<?xml version="1.0" encoding="UTF-8"?>
<dependency-mappings>
    <bean>
        <name>user</name>
        <class>bean.UserBean</class>
        <property>
            <name>userName</name>
        </property>
        <property>
            <name>password</name>
        </property>
        
    </bean>
    <bean>
        <name>login</name>
        <class>servlet.action.LoginAction</class>
        <property>
            <name>userBean</name>
            <ref-class>user</ref-class>
        </property>
    </bean>
</dependency-mappings>