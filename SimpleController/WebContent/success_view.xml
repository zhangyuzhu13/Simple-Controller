<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="login/success.xsl"?>
<view>
	<header>
        <title>a viewer in MVC</title>
    </header>
    <body>
        <form>
            <name>LogoutForm</name>
            <action>logout.action</action>
            <method>post</method>
            <textView>
                <name>userName</name>
                <label>login name:</label>
                <value>java</value>
            </textView>
            <textView>
                <name>userAge</name>
                <label>Age</label>
                <value>18</value>
            </textView>
            <buttonView>
                <name>logoutButton</name>
                <label>Logout</label>
            </buttonView>
        </form>
    </body>
</view>